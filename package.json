{
  "name": "local-felix",
  "version": "1.0.0",
  "description": "A TypeScript TUI chatbot using Ollama, LlamaIndex, and web search capabilities",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "prestart": "npm run build",
    "dev": "tsc && node dist/src/index.js",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "chatbot",
    "ollama",
    "llamaindex",
    "tui",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "llamaindex": "^0.7.0",
    "ollama": "^0.5.0",
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "vitest": "^1.4.0",
    "typescript": "^5.3.0"
  }
}
